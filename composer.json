{
    "name": "yardeight/shoppingcart",
    "description": "A shopping cart for Laravel",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yard8",
            "email": "hello@yard8.co.za"
        }
    ],
    "require": {
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/session": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/events": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0|~6.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "YardEight\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YardEight\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "YardEight\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "YardEight\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
